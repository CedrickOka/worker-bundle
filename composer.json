{
	"name" : "coka/worker-bundle",
	"type" : "symfony-bundle",
	"description" : "Run worker",
	"homepage" : "https://github.com/CedrickOka/worker-bundle",
	"license" : "MIT",
	"keywords" : [
		"worker"
	],
	"authors" : [{
			"name" : "Cedrick Oka",
			"email" : "okacedrick@gmail.com"
		}
	],
	"autoload" : {
		"psr-4" : {
			"Oka\\WorkerBundle\\" : "src"
		},
		"exclude-from-classmap" : [
			"/tests/"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"Oka\\WorkerBundle\\Tests\\" : "tests/"
		}
	},
	"require" : {
		"php" : ">=7.4",
		"symfony/cache" : "^4.4 || ^5.0",
		"symfony/config" : "^4.4 || ^5.0",
		"symfony/console" : "^4.4 || ^5.0",
		"symfony/dependency-injection" : "^4.4 || ^5.0",
		"symfony/event-dispatcher" : "^4.4 || ^5.0",
		"symfony/http-kernel" : "^4.4 || ^5.0"
	},
	"require-dev" : {
		"symfony/framework-bundle" : "^4.4 || ^5.0",
		"symfony/phpunit-bridge" : "^4.4 || ^5.0"
	},
	"config" : {
		"sort-packages" : true
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.x-dev"
		},
		"symfony" : {
			"allow-contrib" : "true"
		}
	},
	"support" : {
		"docs" : "https://github.com/CedrickOka/worker-bundle/blob/master/src/Resources/doc/index.md"
	}
}
